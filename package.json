  },
  "dependencies": {
    "@iobroker/adapter-core": "^3.1.6"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.8.0",
    "@alcalzone/release-script-plugin-iobroker": "^3.7.2",
    "@alcalzone/release-script-plugin-license": "^3.7.0",
    "@alcalzone/release-script-plugin-manual-review": "^3.7.0",
    "@iobroker/adapter-dev": "^1.3.0",
    "@iobroker/testing": "^4.1.3",
    "@iobroker/vis-2-widgets-react-dev": "^3.0.7",
    "@iobroker/vis-2-widgets-testing": "^1.0.4",
    "@tsconfig/node18": "^18.2.4",
    "@types/chai": "^4.3.19",
    "@types/chai-as-promised": "^8.0.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.5.1",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^3.2.12",
    "chai": "^5.1.1",
    "chai-as-promised": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "gulp": "^5.0.0",
    "mocha": "^10.7.3",
    "prettier": "^3.3.3",
    "proxyquire": "^2.1.3",
    "sinon": "^18.0.0",
    "sinon-chai": "^4.0.0",
    "typescript": "~5.6.2"
  },
  "main": "main.js",
  "files": [
    "admin/",
    "img/",
    "lib/",
    "widgets/",
    "www/",
    "io-package.json",
    "LICENSE",
    "main.js"
  ],
  "scripts": {
    "prepublishOnly": "gulp",
    "build": "gulp",
    "test:js": "mocha --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.js,*.test.js,test/**/test!(PackageFiles|Startup).js}\"",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:js && npm run test:package",
    "test1": "mocha ./test/*.test.js",
    "check": "tsc --noEmit -p tsconfig.check.json",
    "lint": "eslint",
    "lint_W": "eslint src-widgets/src/*.jsx",
    "translate": "translate-adapter",
    "release": "release-script",
    "update-packages": "ncu --upgrade && cd src-widgets && ncu --upgrade && cd ..",
    "npm": "npm i && cd src-widgets && npm i -f"
  },
  "bugs": {
    "url": "https://github.com/csicom/ioBroker.csicomtest/issues"
  },
  "readmeFilename": "README.md"
}
